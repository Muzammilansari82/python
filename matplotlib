import matplotlib.pyplot as plt
import numpy as np
import math
x= np.arange(0, math.pi*4, 0.5)
y= np.sin(x)
plt.plot(x,y, "g--")
plt.xlabel("angel1")
plt.ylabel("angel2")
plt.title("Matplot")
# fig=plt.figure()
# ax=fig.add_axes([0,0,1,1])
plt.savefig("abc.jpg")

import numpy as np
import matplotlib.pyplot as plt

x = np.arange(0, math.pi*1, 2)
y = np.sin(x)


plt.plot(x, y, "r-")
plt.xlabel("angle1")
plt.ylabel("angle2")
plt.title("Matplotlib")

plt.savefig("alpha.png")



plt.show()
